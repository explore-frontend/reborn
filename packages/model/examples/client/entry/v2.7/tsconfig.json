{
    "extends": "@kwai-explore/tsconfig",
    "compilerOptions": {
        "lib": ["esnext", "dom"],
        "module": "CommonJS",
        "moduleResolution": "node",
        "noEmit": true,
        "verbatimModuleSyntax": false,
        "paths": {
            "vue": ["../../../../node_modules/vue2.7"],
            "vue/*": ["../../../../node_modules/vue2.7/*"],
            "vue-demi": ["../../../../node_modules/vue-demi/lib/v2.7"]
        }
    },
    "include": ["./**/*"],
    "vueCompilerOptions": {
        "target": 2.7
    }
}
