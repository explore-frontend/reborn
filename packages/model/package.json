{
    "name": "@kwai-explore/model",
    "version": "0.2.0-alpha6",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "typings": "dist/index.d.cts",
    "license": "MIT",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist",
        "src",
        "README.md"
    ],
    "scripts": {
        "type-check-vue3": "tsc --skipLibCheck --project ./tsconfig.lib.vue3.json && vue-tsc --skipLibCheck --project ./examples/client/entry/v3/tsconfig.json",
        "type-check-vue2.7": "tsc --skipLibCheck --project ./tsconfig.lib.vue2.7.json && vue-tsc --skipLibCheck --project ./examples/client/entry/v3/tsconfig.json",
        "type-check": "pnpm run type-check-vue3 && pnpm run type-check-vue2.7",
        "build": "rollup -c ./rollup.config.mjs",
        "serve-vue2.7": "VUE_VERSION=2.7 ts-node --project ./tsconfig.examples.json examples/server/index.ts",
        "serve-vue3": "VUE_VERSION=3 ts-node --project ./tsconfig.examples.json examples/server/index.ts",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "test-vue2.7": "VUE_VERSION=2.7 vitest",
        "test-vue3": "VUE_VERSION=3 vitest",
        "coverage": "VUE_VERSION=2.7 vitest run --coverage && VUE_VERSION=3 vitest run --coverage"
    },
    "dependencies": {
        "graphql": "^16.6.0"
    },
    "peerDependencies": {
        "rxjs": "^7.0.0"
    },
    "publishConfig": {
        "peerDependencies": {
            "vue": "^2.7.0 || ^3.2.0",
            "vue-demi": "^0.14.0",
            "vue-router": "~3.6.0 || ^4.0.14"
        }
    },
    "devDependencies": {
        "@kwai-explore/tsconfig": "^0.0.3",
        "@types/express": "^4.17.17",
        "@types/node": "^18.16.16",
        "@typescript-eslint/parser": "^5.59.8",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vitejs/plugin-vue2": "^2.2.0",
        "@vitest/coverage-c8": "^0.31.1",
        "eslint": "^8.41.0",
        "express": "^4.18.2",
        "jsdom": "^22.1.0",
        "node-fetch": "^3.3.1",
        "prettier": "^2.8.8",
        "rollup": "3.28.1",
        "rollup-plugin-dts": "6.0.0",
        "rollup-plugin-typescript2": "^0.35.0",
        "rxjs": "^7.8.1",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.2",
        "typescript": "^5.0.4",
        "unfetch": "^5.0.0",
        "vite": "^4.3.9",
        "vitest": "^0.31.1",
        "vitest-fetch-mock": "^0.2.2",
        "vue3": "npm:vue@^3.3.4",
        "vue2.7": "npm:vue@~2.7.0",
        "vue-demi": "^0.14.6",
        "vue-router3": "npm:vue-router@~3.6.0",
        "vue-router4": "npm:vue-router@^4.2.2",
        "vue-tsc": "^1.8.8",
        "vuepress": "^1.9.9"
    }
}
