{
    "name": "@kwai-explore/model",
    "version": "0.2.0-alpha1",
    "main": "dist/model.js",
    "module": "dist/model.mjs",
    "typings": "dist/src/index.d.ts",
    "license": "MIT",
    "exports": {
        ".": {
            "import": {
                "types": "./types/esm/index.d.ts",
                "default": "./dist/model.mjs"
            },
            "require": {
                "types": "./types/cjs/index.d.ts",
                "default": "./dist/model.js"
            }
        }
    },
    "files": [
        "dist",
        "types",
        "src",
        "README.md"
    ],
    "scripts": {
        "type-check": "vue-tsc --project ./tsconfig.test.json && vue-tsc --project ./tsconfig.examples.json",
        "build": "shx rm -rf dist types && vite build && tsc --project ./tsconfig.lib.cjs.json && tsc --project ./tsconfig.lib.esm.json && ts-add-js-extension --dir=./types/esm && shx rm ./types/esm/*.js ./types/esm/**/*.js && shx cp ./src/package.json ./types/esm/package.json",
        "serve": "ts-node --project ./tsconfig.examples.json examples/server/index.ts",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "graphql": "^16.6.0"
    },
    "peerDependencies": {
        "rxjs": "^7.0.0",
        "vue": "^3.2.31",
        "vue-router": "^4.0.14"
    },
    "devDependencies": {
        "@kwai-explore/tsconfig": "^0.0.3",
        "@types/express": "^4.17.17",
        "@types/node": "^18.16.16",
        "@typescript-eslint/parser": "^5.59.8",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vitest/coverage-c8": "^0.31.1",
        "eslint": "^8.41.0",
        "express": "^4.18.2",
        "jsdom": "^22.1.0",
        "node-fetch": "^3.3.1",
        "prettier": "^2.8.8",
        "rxjs": "^7.8.1",
        "shx": "^0.3.4",
        "ts-add-js-extension": "^1.5.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.2",
        "typescript": "^5.0.4",
        "unfetch": "^5.0.0",
        "vite": "^4.3.9",
        "vitest": "^0.31.1",
        "vitest-fetch-mock": "^0.2.2",
        "vue": "^3.3.4",
        "vue-router": "^4.2.2",
        "vue-tsc": "^1.7.8",
        "vuepress": "^1.9.9"
    }
}
